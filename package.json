{
  "name": "raylib",
  "version": "0.1.1",
  "description": "Node.js bindings for raylib.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "pretest": "tsc test/test.ts",
    "test": "mocha",
    "postinstall": "cmake-js compile",
    "clean": "rm -rf build",
    "compile": "cmake-js compile",
    "prepkg": "npm run compile && tsc",
    "pkg": "node src/scripts/pkg.js",
    "dev": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobLoach/node-raylib.git"
  },
  "engines": {
    "node": ">=10"
  },
  "bin": "bin/node-raylib",
  "keywords": [
    "raylib"
  ],
  "author": "Rob Loach",
  "license": "Zlib",
  "files": [
    "bin/*",
    "src/*",
    "lib/*",
    "src/lib/*",
    "CMakeLists.txt",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "bugs": {
    "url": "https://github.com/RobLoach/node-raylib/issues"
  },
  "homepage": "https://github.com/RobLoach/node-raylib",
  "dependencies": {
    "bindings": "^1.5.0",
    "cmake-js": "^5.1.0",
    "meow": "^5.0.0",
    "module-alias": "^2.2.0",
    "node-addon-api": "^1.6.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "archiver": "^3.0.0",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "pkg": "^4.3.7",
    "typescript": "^3.5.3"
  }
}
